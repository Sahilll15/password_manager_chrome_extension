{
    "manifest_version": 3,
    "name": "Password Manager",
    "version": "1.0",
    "description": "A password manager extension",
    "permissions": [
      "storage",
      "activeTab"
    ],
    "action": {
      "default_popup": "popup.html"
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  